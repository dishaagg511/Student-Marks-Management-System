@model IEnumerable<WebApplication8.Models.mark>

@{
    ViewBag.Title = "Index";
}

<p>
    <a href="@Url.Action("Create")" class="btn btn-success">Create New</a>

</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.student.Candidate_Name)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Subject_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Marks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudentId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.student.Candidate_Name)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Subject_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Marks)
        </td>
        <td>

            <a href="@Url.Action("Edit", "Marks", new { studentId = item.StudentId, subjectId = item.SubjectId })" class="btn btn-primary">Edit</a>
            <a href="@Url.Action("Delete", "Marks", new { studentId = item.StudentId, subjectId = item.SubjectId })" class="btn btn-danger">Delete</a>

        </td>
    </tr>
}

</table>
